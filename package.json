{
  "name": "face-quiz",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "test:headless": "ng test --browsers ChromeHeadless --watch=false --code-coverage",
    "lint": "ng lint",
    "build:github-pages": "ng build --prod --base-href \"https://ilseh.github.io/face-quiz/\"",
    "deploy:github-pages": "ngh -d ./dist/face-quiz/  -r https://GH_TOKEN@github.com/ilseh/face-quiz.git",
    "e2e": "ng e2e",
    "fix": "echo \"module.exports = require('readable-stream');\" > ./node_modules/jszip/lib/readable-stream-browser.js",
    "postinstall": "ngcc --properties es2015 browser module main --first-only --create-ivy-entry-points"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^11.0.0-next.4",
    "@angular/cdk": "^10.2.3",
    "@angular/common": "^11.0.0-next.4",
    "@angular/compiler": "^11.0.0-next.4",
    "@angular/core": "^11.0.5",
    "@angular/forms": "^11.0.0-next.4",
    "@angular/material": "^10.2.3",
    "@angular/platform-browser": "^11.0.0-next.4",
    "@angular/platform-browser-dynamic": "^11.0.0-next.4",
    "@angular/router": "^11.0.0-next.4",
    "@types/jszip": "^3.1.6",
    "@types/lodash": "^4.14.138",
    "core-js": "^2.6.9",
    "file-saver": "^2.0.2",
    "file-system": "^2.2.2",
    "jszip": "^3.2.2",
    "lodash": "^4.17.19",
    "ngx-take-until-destroy": "^5.4.0",
    "rxjs": "^6.5.4",
    "stream": "0.0.2",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.1001.4",
    "@angular/cli": "^10.1.4",
    "@angular/compiler-cli": "^11.0.0-next.4",
    "@angular/language-service": "^11.0.0-next.4",
    "@ngneat/spectator": "^5.13.4",
    "@types/jasmine": "^2.8.16",
    "@types/jasminewd2": "^2.0.6",
    "@types/node": "^12.11.1",
    "codelyzer": "^5.1.2",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "protractor": "~7.0.0",
    "ts-node": "~7.0.0",
    "tslint": "~6.1.0",
    "typescript": "~4.0.3"
  },
  "browser": {
    "fs": false,
    "path": false,
    "os": false
  }
}
